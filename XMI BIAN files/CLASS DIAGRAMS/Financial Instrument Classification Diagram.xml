   <Column name="Name" value="EquityInstrument"/>
   <Column name="Note" value="Any contract that evidences a residual interest in the assets of an entity after deducting all of its liabilities.
https://www.iasplus.com/en/standards/ias/ias32"/>
   <Column name="Backcolor" value="16777215"/>
   <Column name="ea_guid" value="{e057663d-bba4-ea11-829f-0660fb1fa966}"/>
